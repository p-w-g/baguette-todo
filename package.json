{
  "name": "baguette-todo",
  "version": "1.0.0",
  "description": "Classic todo app with a JS OOP twist.",
  "main": "main.js",
  "scripts": {
    "lint": "eslint ./js/*.mjs",
    "lint-f-mjs": "eslint ./js/*.mjs --fix",
    "lint-f-js": "eslint *.js --fix",
    "start": "electron .",
    "pack-win": "electron-packager . baguette-todo --overwrite --asar=true --platform=win32 --arch=ia32 --icon=favicon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"baguette-todo\"",
    "pack-osx": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=favicon.ico --prune=true --out=release-builds"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sltPrz/baguette-todo.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sltPrz/baguette-todo/issues"
  },
  "homepage": "https://github.com/sltPrz/baguette-todo#readme",
  "devDependencies": {
    "electron": "^9.4.0",
    "electron-packager": "^13.1.1",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4"
  },
  "dependencies": {}
}
